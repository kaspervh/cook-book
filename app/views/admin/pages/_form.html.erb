<%= form_for @page do |f| %>
  <%= f.label :vælg_emne %>
  <br>
  <%= f.collection_select(:admin_subject_id, @admin_subjects, :id,:name ) %>
  <br>
  <%= f.label :headder %>
  <br>
  <%= f.text_field :headder %>
  <br>
  <%= f.label :content %>
  <br>
  <%= f.cktext_area :content, class: "ckeditor" %>
  <br>
  <br>
  <%= f.submit " tilføj side " %>
<% end %>
<%= link_to " Tilbage ", admin_pages_path %>